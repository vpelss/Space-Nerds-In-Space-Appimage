#!/bin/bash
SELF=$(readlink -f "$0") #will set SELF to our AppRun path with AppRun appended 
echo ${SELF}
HERE=${SELF%/*} #will set HERE to path without AppRun
echo ${HERE}
EXEC="${HERE}/usr" #set path to bin folder. eg: /tmp/.mount_Bridge6DxuEO/usr/bin
cd ${EXEC}
echo ${EXEC}
#export SNIS_LOG_DIR=~/.local/share/space-nerds-in-space/snis_log_dir
#mkdir -p $SNIS_LOG_DIR a
#export SNIS_DB_DIR=~/.local/share/space-nerds-in-space/snis_db_dir
#mkdir -p $SNIS_DB_DIR
exec bin/snis_launcher  #run our app
